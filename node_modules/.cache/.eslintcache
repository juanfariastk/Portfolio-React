o(texto_novo)\r\n\r\n        if(deleta_estado){\r\n            set_estado(temp_anterior => temp_anterior/2)\r\n        }\r\n\r\n        if(!deleta_estado && texto_novo === texto_completo){\r\n            set_delete(true)\r\n            set_estado(tempo_animacao)\r\n        }else if(deleta_estado && texto_novo ===''){\r\n            set_delete(false)\r\n            set_loop(loop_items + 1)\r\n            set_estado(500)\r\n        }\r\n    }\r\n    return (\r\n        <section className=\"banner\" id=\"home\">\r\n            <Container>\r\n                <Row className=\"align-items-center\">\r\n                    <Col xs={12}  md={6} xl={7}>\r\n                        <span className=\"tagline\">Seja Bem-Vindo ao meu Mundo!</span>\r\n                        <h1>{`Olá! Me chamo Juan `}<span className=\"wrap\">{texto_atual}</span></h1>\r\n                        <p>Desça e conheça um pouco mais sobre os meus projetos...</p>\r\n                    </Col>\r\n                    <Col xs={12} md={6} xl={5}>\r\n                        <img src={yone_banner} alt=\"banner_inicio\"/>\r\n                    </Col>\r\n                </Row>\r\n            </Container>\r\n        </section>\r\n    )\r\n}","C:\\Users\\Juan Farias\\portfolio-pessoal\\src\\componentes\\Habilidades.js",["39","40"],[],{"ruleId":"41","severity":1,"message":"42","line":1,"column":8,"nodeType":"43","messageId":"44","endLine":1,"endColumn":12},{"ruleId":"45","severity":1,"message":"46","line":43,"column":21,"nodeType":"47","messageId":"48","endLine":43,"endColumn":88,"fix":"49"},{"ruleId":"45","severity":1,"message":"46","line":44,"column":21,"nodeType":"47","messageId":"48","endLine":44,"endColumn":89,"fix":"50"},{"ruleId":"51","severity":1,"message":"52","line":20,"column":9,"nodeType":"53","endLine":20,"endColumn":22,"suggestions":"54"},{"ruleId":"41","severity":1,"message":"55","line":8,"column":8,"nodeType":"43","messageId":"44","endLine":8,"endColumn":22},{"ruleId":"56","severity":1,"message":"57